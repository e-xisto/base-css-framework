{
	"name": "base-css-framework",
	"version": "0.1.1",
	"description": "Base CSS Framework & Styleguide",
	"scripts": {
		"dev": "npx npm-run-all --serial del dev:webpack",
		"build": "npx npm-run-all --serial del prod:webpack",
		"dev:webpack": "npx webpack --watch --mode=development",
		"prod:webpack": "npx webpack --mode=production",
		"del": "rimraf dist && rimraf docs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/e-xisto/base-css-framework.git"
	},
	"keywords": [
		"base",
		"css",
		"framework",
		"sass"
	],
	"author": "Juan Miguel Guerrero <juanmiguel@existo.es> (https://www.existo.es)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/e-xisto/base-css-framework/issues"
	},
	"homepage": "https://github.com/e-xisto/base-css-framework#readme",
	"dependencies": {
		"simple-parallax-js": "^5.3.0"
	},
	"devDependencies": {
		"@types/swiper": "^5.2.1",
		"browser-sync": "^2.26.7",
		"browser-sync-webpack-plugin": "^2.2.2",
		"copy-dir": "^1.2.0",
		"css-loader": "^1.0.0",
		"event-hooks-webpack-plugin": "^2.1.5",
		"extra-watch-webpack-plugin": "^1.0.3",
		"extract-loader": "^3.0.0",
		"file-loader": "^2.0.0",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"jarallax": "^1.12.0",
		"marked": "^0.8.0",
		"mini-css-extract-plugin": "^0.4.3",
		"node-sass": "^4.13.1",
		"npm-run-all": "^4.1.5",
		"nunjucks": "^3.1.3",
		"nunjucks-markdown": "^2.0.1",
		"nunjucks-webpack-plugin": "^5.0.0",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.0",
		"swiper": "^5.3.0",
		"terser-webpack-plugin": "^2.3.4",
		"ts-loader": "^5.2.1",
		"typescript": "^3.7.5",
		"url-loader": "^1.1.1",
		"webpack": "^4.20.2",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.10.2"
	}
}
